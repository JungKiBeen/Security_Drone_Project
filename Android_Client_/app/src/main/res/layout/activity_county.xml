    <Button android:id="@+id/county_btn7" android:layout_width="match_parent" android:background="#a9a9a9" android:textColor="#fff"  android:textStyle="bold" android:layout_weight="1" android:layout_height="0dp" android:layout_marginTop="1dp" android:text="Gyeongsang-do" android:textSize="30dp"/>
    <Button android:id="@+id/county_btn8" android:layout_width="match_parent" android:background="#a9a9a9" android:textColor="#fff"  android:textStyle="bold" android:layout_weight="1" android:layout_height="0dp" android:layout_marginTop="1dp" android:text="Jeju" android:textSize="30dp"/>
